<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Title</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.citation>
                        <TCEforms>
                            <label>Citation du bloc</label>
                            <config>
                                <type>text</type>
                                <size>10</size>
                                <rows>5</rows>
                            </config>
                        </TCEforms>
                    </settings.citation>

                    <settings.signature>
                        <TCEforms>
                            <label>Signature de la citation</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                            </config>
                        </TCEforms>
                    </settings.signature>

                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>