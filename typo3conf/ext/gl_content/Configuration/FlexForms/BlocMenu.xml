<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Title</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>

                    <settings.titre>
                        <TCEforms>
                            <label>Titre du bloc</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                            </config>
                        </TCEforms>
                    </settings.titre>
                    
                    <settings.menus>
                        <type>array</type>
                        <section>1</section>
                        <label>Liste des menus</label>
                        <el>
                            <menu type="array">
                                <type>array</type>
                                <el>

                                    <titre-menu>
                                        <TCEforms>
                                            <label>Titre du menu</label>
                                            <config>
                                                <type>input</type>
                                                <size>30</size>
                                            </config>
                                        </TCEforms>
                                    </titre-menu>

                                </el>
                            </menu>
                        </el>
                    </settings.menus>

                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>